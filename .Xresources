! These options only take effect on startup.
st.termname:     st-256color
! if you do not set shell, precedence is: -e arg, utmp option, SHELL env var, /etc/passwd shell
! st.shell:        /bin/sh

! The following options options can be reloaded via USR1 signal.
st.font:         Liberation Mono:pixelsize=12:antialias=true:autohint=true;
st.borderpx:     10
! Available cursor values: 2 4 6 7 = █ _ | ☃ ( 1 3 5 are blinking versions)
st.cursorshape:  2
st.cursorthickness:  2
st.bold_font:    0
st.xfps:         120
st.actionfps:    30
st.chscale:      1.0
st.cwscale:      1.0
st.blinktimeout: 800
st.bellvolume:   0
! this is a char that is exposed like so: `printf '\033[z'`
st.prompt_char:  $

! special
st.foreground:   #e6e1dc
st.background:   #2b2b2b
st.cursorColor:  #e6e1dc

! black
st.color0:       #2b2b2b
st.color8:       #5a647e

! red
st.color1:       #da4939
st.color9:       #da4939

! green
st.color2:       #a5c261
st.color10:      #a5c261

! yellow
st.color3:       #ffc66d
st.color11:      #ffc66d

! blue
st.color4:       #6d9cbe
st.color12:      #6d9cbe

! magenta
st.color5:       #b6b3eb
st.color13:      #b6b3eb

! cyan
st.color6:       #519f50
st.color14:      #519f50

! white
st.color7:       #e6e1dc
st.color15:      #f9f7f3

